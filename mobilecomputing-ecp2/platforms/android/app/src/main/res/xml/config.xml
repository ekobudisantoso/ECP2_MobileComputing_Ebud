<?xml version='1.0' encoding='utf-8'?>
<widget id="com.ebud.ecp2" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <name>ECP2RANDY</name>
    <description>
        Belajar Cordova
    </description>
    <author email="ebud.santoso@gmail.com" href="">
        Eko Budi Santoso - 04315047
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-intent href="market:*" />
    <icon density="ldpi" src="www/img/logo.jpg" />
    <icon density="mdpi" src="www/img/logo.jpg" />
    <icon density="hdpi" src="www/img/logo.jpg" />
    <icon density="xhdpi" src="www/img/logo.jpg" />
    <icon density="xxhdpi" src="www/img/logo.jpg" />
    <icon density="xxxhdpi" src="www/img/logo.jpg" />
    <preference name="loglevel" value="DEBUG" />
</widget>
